{
  "name": "react-jest-tdd",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "enzyme": "^2.9.1",
    "jsdom": "^11.0.0",
    "react-addons-test-utils": "^15.6.0",
    "react-scripts": "1.0.10",
    "react-test-renderer": "^15.6.1"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
